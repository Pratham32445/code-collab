generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Space {
  Id              String @id @default(uuid())
  spaceName       String
  subscribedUsers User[]
}

model User {
  Id       String @id @default(uuid())
  email    String
  password String
  spaceId  String
  space    Space  @relation(fields: [spaceId], references: [Id])
}
